{
  "version": "1.0.0-*",
  "description": "Shared Entity Framework components for relational data stores.",
  "compilationOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.EntityFrameworkCore": "1.0.0-*"
  },
  "compile": "../Shared/*.cs",
  "resourceFiles": "Properties/Microsoft.EntityFrameworkCore.Relational.rd.xml",
  "frameworks": {
    "net451": {
      "dependencies": {
        "System.Diagnostics.DiagnosticSource": "4.0.0-*"
      },
      "frameworkAssemblies": {
        "System.Data": "",
        "System.Diagnostics.Tracing": {
          "type": "build"
        },
        "System.Runtime": {
          "type": "build"
        },
        "System.Threading": {
          "type": "build"
        },
        "System.Transactions": ""
      }
    },
    "netstandard1.3": {
      "imports": [
        "portable-net452+win81"
      ],
      "dependencies": {
        "Microsoft.CSharp": "4.0.1-*",
        "System.Data.Common": "4.0.1-*",
        "System.Diagnostics.DiagnosticSource": "4.0.0-*",
        "System.Text.RegularExpressions": "4.0.12-*"
      }
    },
    "netcore50": {
      "dependencies": {
        "Microsoft.NETCore.Platforms": {
          "type": "build",
          "version": "1.0.1-*"
        },
        "Microsoft.CSharp": "4.0.0",
        "System.Data.Common": "4.0.0",
        "System.Text.RegularExpressions": "4.0.10",
        "System.Diagnostics.DiagnosticSource": "4.0.0-beta-*",
        "System.Dynamic.Runtime": {
          "type": "build",
          "version": "4.0.10"
        },
        "System.Runtime.Extensions": {
          "type": "build",
          "version": "4.0.10"
        }
      }
    }
  }
}