{
  "version": "1.0.0-*",
  "description": "Reusable Microsoft.EntityFrameworkCore.Relational.Design functional tests for provider writers.",
  "compilationOptions": {
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true,
    "preserveCompilationContext": true
  },
  "dependencies": {
    "Microsoft.EntityFrameworkCore.Commands": "1.0.0-*",
    "Microsoft.EntityFrameworkCore.Relational.Design": "1.0.0-*",
    "Microsoft.EntityFrameworkCore.Relational.Specification.Tests": "1.0.0-*",
    "Microsoft.Extensions.DependencyModel": "1.0.0-*",
    "Microsoft.CodeAnalysis.CSharp": "1.3.0-*"
  },
  "frameworks": {
    "netstandard1.3": {
      "imports": [
        "dnxcore50",
        "portable-net452+win81"
      ],
      "dependencies": {
        "System.Threading.Tasks.Parallel": "4.0.1-*"
      }
    },
    "net451": {
      "frameworkAssemblies": {
        "System.Collections.Concurrent": {
          "type": "build"
        },
        "System.Diagnostics.Tools": {
          "type": "build"
        },
        "System.Dynamic.Runtime": {
          "type": "build"
        },
        "System.Reflection.Primitives": {
          "type": "build"
        },
        "System.Runtime.Numerics": {
          "type": "build"
        },
        "System.Runtime.Serialization.Json": {
          "type": "build"
        },
        "System.Text.Encoding": {
          "type": "build"
        },
        "System.Text.Encoding.Extensions": {
          "type": "build"
        },
        "System.Threading.Tasks.Parallel": {
          "type": "build"
        },
        "System.Xml.ReaderWriter": {
          "type": "build"
        },
        "System.Xml.XDocument": {
          "type": "build"
        }
      }
    }
  }
}